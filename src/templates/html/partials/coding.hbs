<h2>{{ coding.title }}</h2>
<dl>
  {{#iterateObject "coding" this}}
    <dt>{{ key }}</dt>
    <dd>{{{m value }}}</dd>
  {{!-- Ergo, if value is iterable --}}
  {{else}}
    <dt>{{ key }}</dt>
    <dd>
      <ul>
        {{#each arr as |value|}}
          <li>{{{m value}}}</li>
        {{/each}}
      </ul>
    </dd>
  {{/iterateObject}}
</dl>